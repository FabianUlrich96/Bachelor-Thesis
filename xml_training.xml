                        &lt;xs:documentation&gt;Search/checking criteria&lt;/xs:documentation&gt;
                    &lt;/xs:annotation&gt;
                    &lt;xs:complexType&gt;
                        &lt;xs:sequence&gt;
                            &lt;xs:element name="bookingCriteria"&gt;
                                &lt;xs:annotation&gt;
                                    &lt;xs:documentation&gt;Critieria relating to the booking that is the basis of the search&lt;/xs:documentation&gt;
                                &lt;/xs:annotation&gt;
                                &lt;xs:complexType&gt;
                                    &lt;xs:sequence&gt;
                                        &lt;xs:element name="productType"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;Type of product required&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:simpleType&gt;
                                                &lt;xs:restriction base="xs:string"&gt;
                                                    &lt;xs:length value="1"/&gt;
                                                    &lt;xs:enumeration value="3"/&gt;
                                                &lt;/xs:restriction&gt;
                                            &lt;/xs:simpleType&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:choice&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;Only one of the following values can be used&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:element name="cityNumbers"&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:sequence maxOccurs="10"&gt;
                                                        &lt;xs:element name="city"&gt;
                                                            &lt;xs:complexType&gt;
                                                                &lt;xs:sequence&gt;
                                                                    &lt;xs:element name="cityLocations" minOccurs="0"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;If this element exists, only products from these locations will be returned&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:complexType&gt;
                                                                            &lt;xs:sequence maxOccurs="5"&gt;
                                                                                &lt;xs:element name="cityLocation"&gt;
                                                                                    &lt;xs:annotation&gt;
                                                                                        &lt;xs:documentation&gt;9 digit Integer&lt;/xs:documentation&gt;
                                                                                    &lt;/xs:annotation&gt;
                                                                                    &lt;xs:simpleType&gt;
                                                                                        &lt;xs:restriction base="xs:integer"&gt;
                                                                                            &lt;xs:pattern value=""/&gt;
                                                                                        &lt;/xs:restriction&gt;
                                                                                    &lt;/xs:simpleType&gt;
                                                                                &lt;/xs:element&gt;
                                                                            &lt;/xs:sequence&gt;
                                                                        &lt;/xs:complexType&gt;
                                                                    &lt;/xs:element&gt;
                                                                &lt;/xs:sequence&gt;
                                                                &lt;xs:attribute name="cityNumber" use="required"&gt;
                                                                    &lt;xs:simpleType&gt;
                                                                        &lt;xs:restriction base="xs:integer"&gt;
                                                                            &lt;xs:totalDigits value="5"/&gt;
                                                                            &lt;xs:minInclusive value="1"/&gt;
                                                                        &lt;/xs:restriction&gt;
                                                                    &lt;/xs:simpleType&gt;
                                                                &lt;/xs:attribute&gt;
                                                            &lt;/xs:complexType&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:sequence&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
                                            &lt;xs:element name="cityCode"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;Character reference to city (not all cities have this reference)&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:simpleType&gt;
                                                    &lt;xs:restriction base="xs:string"&gt;
                                                        &lt;xs:length value="3"/&gt;
                                                    &lt;/xs:restriction&gt;
                                                &lt;/xs:simpleType&gt;
                                            &lt;/xs:element&gt;
                                            &lt;xs:element name="areaNames"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;Reference to area(s), or group of cities defined by Miki&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:sequence maxOccurs="5"&gt;
                                                        &lt;xs:element name="areaName"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;Reference to an area, or group of cities defined by Miki&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                            &lt;xs:simpleType&gt;
                                                                &lt;xs:restriction base="xs:string"&gt;
                                                                    &lt;xs:minLength value="1"/&gt;
                                                                    &lt;xs:maxLength value="10"/&gt;
                                                                &lt;/xs:restriction&gt;
                                                            &lt;/xs:simpleType&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:sequence&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
                                            &lt;xs:element name="airportCodes"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;FOR LATER USE&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:sequence maxOccurs="5"&gt;
                                                        &lt;xs:element name="airportCode"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;FOR LATER USE&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                            &lt;xs:simpleType&gt;
                                                                &lt;xs:restriction base="xs:string"&gt;
                                                                    &lt;xs:length value="3"/&gt;
                                                                &lt;/xs:restriction&gt;
                                                            &lt;/xs:simpleType&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:sequence&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
&lt;xs:element name="productCodes"&gt;
    &lt;xs:annotation&gt;
        &lt;xs:documentation&gt;Multiple products can be specified&lt;/xs:documentation&gt;
    &lt;/xs:annotation&gt;
    &lt;xs:complexType&gt;
        &lt;xs:sequence maxOccurs="50"&gt;
            &lt;xs:element name="productCode"&gt;
                &lt;xs:annotation&gt;
                    &lt;xs:documentation&gt;Unique reference to product&lt;/xs:documentation&gt;
                &lt;/xs:annotation&gt;
                &lt;xs:simpleType&gt;
                    &lt;xs:restriction base="xs:string"&gt;
                        &lt;xs:length value="9"/&gt;
                    &lt;/xs:restriction&gt;
                &lt;/xs:simpleType&gt;
            &lt;/xs:element&gt;
        &lt;/xs:sequence&gt;
    &lt;/xs:complexType&gt;
&lt;/xs:element&gt;
                                            &lt;xs:element name="geoLocation"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;Units of measurement of attributes latitude and longitude is degrees. Attribute distance is in metres&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:attribute name="latitude" use="required"&gt;
                                                        &lt;xs:simpleType&gt;
                                                            &lt;xs:restriction base="xs:decimal"&gt;
                                                                &lt;xs:totalDigits value="15"/&gt;
                                                                &lt;xs:fractionDigits value="6"/&gt;
                                                            &lt;/xs:restriction&gt;
                                                        &lt;/xs:simpleType&gt;
                                                    &lt;/xs:attribute&gt;
                                                    &lt;xs:attribute name="longitude" use="required"&gt;
                                                        &lt;xs:simpleType&gt;
                                                            &lt;xs:restriction base="xs:decimal"&gt;
                                                                &lt;xs:totalDigits value="15"/&gt;
                                                                &lt;xs:fractionDigits value="6"/&gt;
                                                            &lt;/xs:restriction&gt;
                                                        &lt;/xs:simpleType&gt;
                                                    &lt;/xs:attribute&gt;
                                                    &lt;xs:attribute name="distance" type="xs:decimal" use="required"/&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
                                        &lt;/xs:choice&gt;
                                        &lt;xs:element name="bookingDate" type="xs:date"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;The date that the search is being made&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:element name="serviceDate" type="xs:date"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;The date for which availability is being checked (check-in date for hotel products)&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:element name="numberOfNights" minOccurs="0"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;If the product type is "hotel", then nights must be specified&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:simpleType&gt;
                                                &lt;xs:restriction base="xs:integer"&gt;
                                                    &lt;xs:minInclusive value="1"/&gt;
                                                    &lt;xs:maxInclusive value="30"/&gt;
                                                &lt;/xs:restriction&gt;
                                            &lt;/xs:simpleType&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:choice&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;Only one of the following values can be used&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:element name="subProducts"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;Request by room type - multiple sub products can be requested&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:sequence maxOccurs="5"&gt;
                                                        &lt;xs:element name="subProduct"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;Each sub product needs to be specified with a code and quantity&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                            &lt;xs:complexType&gt;
                                                                &lt;xs:sequence&gt;
                                                                    &lt;xs:element name="subProductCode"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;The code representing the sub product for which the availability check has been requested&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:simpleType&gt;
                                                                            &lt;xs:restriction base="xs:int"&gt;
                                                                                &lt;xs:minInclusive value="1"/&gt;
                                                                                &lt;xs:maxInclusive value="99999"/&gt;
                                                                            &lt;/xs:restriction&gt;
                                                                        &lt;/xs:simpleType&gt;
                                                                    &lt;/xs:element&gt;
                                                                    &lt;xs:element name="subProductQuantity"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;How many of each sub product are required&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:simpleType&gt;
                                                                            &lt;xs:restriction base="xs:integer"&gt;
                                                                                &lt;xs:minInclusive value="1"/&gt;
                                                                                &lt;xs:maxInclusive value="999"/&gt;
                                                                            &lt;/xs:restriction&gt;
                                                                        &lt;/xs:simpleType&gt;
                                                                    &lt;/xs:element&gt;
                                                                &lt;/xs:sequence&gt;
                                                                &lt;xs:attribute name="exactMatch" type="xs:boolean" use="optional" default="false"/&gt;
                                                            &lt;/xs:complexType&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:sequence&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
                                            &lt;xs:element name="roomBasis"&gt;
                                                &lt;xs:annotation&gt;
                                                    &lt;xs:documentation&gt;Request by room basis&lt;/xs:documentation&gt;
                                                &lt;/xs:annotation&gt;
                                                &lt;xs:complexType&gt;
                                                    &lt;xs:sequence maxOccurs="5"&gt;
                                                        &lt;xs:element name="room"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;Each roomBasis needs to be specified with a sequence, number of adults and number of children&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                            &lt;xs:complexType&gt;
                                                                &lt;xs:sequence&gt;
                                                                    &lt;xs:element name="roomNumber"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;Identification allowing multiple room basis requirements to be specified&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:simpleType&gt;
                                                                            &lt;xs:restriction base="xs:int"&gt;
                                                                                &lt;xs:minInclusive value="1"/&gt;
                                                                                &lt;xs:maxInclusive value="99999"/&gt;
                                                                            &lt;/xs:restriction&gt;
                                                                        &lt;/xs:simpleType&gt;
                                                                    &lt;/xs:element&gt;
                                                                    &lt;xs:element name="numberOfAdults"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;Number of adults&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:simpleType&gt;
                                                                            &lt;xs:restriction base="xs:integer"&gt;
                                                                                &lt;xs:minInclusive value="1"/&gt;
                                                                                &lt;xs:maxInclusive value="999"/&gt;
                                                                            &lt;/xs:restriction&gt;
                                                                        &lt;/xs:simpleType&gt;
                                                                    &lt;/xs:element&gt;
                                                                    &lt;xs:element name="children" minOccurs="0"&gt;
                                                                        &lt;xs:annotation&gt;
                                                                            &lt;xs:documentation&gt;Children&lt;/xs:documentation&gt;
                                                                        &lt;/xs:annotation&gt;
                                                                        &lt;xs:complexType&gt;
                                                                            &lt;xs:sequence maxOccurs="2"&gt;
                                                                                &lt;xs:element name="child"&gt;
                                                                                    &lt;xs:complexType&gt;
                                                                                        &lt;xs:attribute name="childNumber" use="required"&gt;
                                                                                            &lt;xs:simpleType&gt;
                                                                                                &lt;xs:restriction base="xs:positiveInteger"&gt;
                                                                                                    &lt;xs:minInclusive value="1"/&gt;
                                                                                                    &lt;xs:maxInclusive value="2"/&gt;
                                                                                                &lt;/xs:restriction&gt;
                                                                                            &lt;/xs:simpleType&gt;
                                                                                        &lt;/xs:attribute&gt;
                                                                                        &lt;xs:attribute name="age" use="required"&gt;
                                                                                            &lt;xs:simpleType&gt;
                                                                                                &lt;xs:restriction base="xs:integer"&gt;
                                                                                                    &lt;xs:minInclusive value="0"/&gt;
                                                                                                    &lt;xs:maxInclusive value="12"/&gt;
                                                                                                &lt;/xs:restriction&gt;
                                                                                            &lt;/xs:simpleType&gt;
                                                                                        &lt;/xs:attribute&gt;
                                                                                    &lt;/xs:complexType&gt;
                                                                                &lt;/xs:element&gt;
                                                                            &lt;/xs:sequence&gt;
                                                                        &lt;/xs:complexType&gt;
                                                                        &lt;xs:unique name="childNumber"&gt;
                                                                            &lt;xs:selector xpath="child"/&gt;
                                                                            &lt;xs:field xpath="@childNumber"/&gt;
                                                                        &lt;/xs:unique&gt;
                                                                    &lt;/xs:element&gt;
                                                                &lt;/xs:sequence&gt;
                                                            &lt;/xs:complexType&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:sequence&gt;
                                                &lt;/xs:complexType&gt;
                                            &lt;/xs:element&gt;
                                        &lt;/xs:choice&gt;
                                        &lt;xs:element name="paxDomicile"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;ISO country code for pax main country of residence&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:simpleType&gt;
                                                &lt;xs:restriction base="xs:string"&gt;
                                                    &lt;xs:length value="2"/&gt;
                                                &lt;/xs:restriction&gt;
                                            &lt;/xs:simpleType&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:element name="salesChannel" minOccurs="0"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;Via what sales channel will the hotel be sold: 1=Wholesale, 2=Retail, 3=Corporate, 4=To consumer, 9=Staff&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:simpleType&gt;
                                                &lt;xs:restriction base="xs:string"&gt;
                                                    &lt;xs:length value="1"/&gt;
                                                    &lt;xs:enumeration value="1"/&gt;
                                                    &lt;xs:enumeration value="2"/&gt;
                                                    &lt;xs:enumeration value="3"/&gt;
                                                &lt;/xs:restriction&gt;
                                            &lt;/xs:simpleType&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:element name="languageCode" minOccurs="0"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;ISO language code for results (if not supported by Miki, en will be used)&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:simpleType&gt;
                                                &lt;xs:restriction base="xs:string"&gt;
                                                    &lt;xs:length value="2"/&gt;
                                                &lt;/xs:restriction&gt;
                                            &lt;/xs:simpleType&gt;
                                        &lt;/xs:element&gt;
                                        &lt;xs:element name="agentMarkup" minOccurs="0"&gt;
                                            &lt;xs:annotation&gt;
                                                &lt;xs:documentation&gt;Agent markup. Only for Miki internal use&lt;/xs:documentation&gt;
                                            &lt;/xs:annotation&gt;
                                            &lt;xs:complexType&gt;
                                                &lt;xs:sequence&gt;
                                                    &lt;xs:choice&gt;
                                                        &lt;xs:element name="amount" type="t_amount"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;Agent commission in numbers&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                        &lt;/xs:element&gt;
                                                        &lt;xs:element name="percentage" type="xs:decimal"&gt;
                                                            &lt;xs:annotation&gt;
                                                                &lt;xs:documentation&gt;Agent commission in percentage&lt;/xs:documentation&gt;
                                                            &lt;/xs:annotation&gt;
                                                        &lt;/xs:element&gt;
                                                    &lt;/xs:choice&gt;
                                                &lt;/xs:sequence&gt;
                                            &lt;/xs:complexType&gt;
                                        &lt;/xs:element&gt;
                                    &lt;/xs:sequence&gt;
                                    &lt;xs:attribute name="currencyCode" type="xs:string"&gt;
                                        &lt;xs:annotation&gt;
                                            &lt;xs:documentation&gt;Currency to be used for search response&lt;/xs:documentation&gt;
                                        &lt;/xs:annotation&gt;
                                    &lt;/xs:attribute&gt;
                                &lt;/xs:complexType&gt;
                            &lt;/xs:element&gt;
                        &lt;/xs:sequence&gt;
                    &lt;/xs:complexType&gt;
                &lt;/xs:element&gt;
            &lt;/xs:sequence&gt;
        &lt;/xs:complexType&gt;
    &lt;/xs:element&gt;
&lt;/xs:schema&gt;

