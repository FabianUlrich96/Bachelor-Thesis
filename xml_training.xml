      &lt;command&gt;nextDialogue = 104&lt;/command&gt;
    &lt;/choice&gt;
    &lt;choice condition="hero.carFixingSkill &lt;= 5" id="I can't..."&gt;
      &lt;command&gt;nextDialogue = 105&lt;/command&gt;
    &lt;/choice&gt;
    &lt;choice id="Fix it yourself"&gt;
