    delete $xml-&gt;{DESIGN_ID}[0]{PROCESS_ID}[0]{RegisterList}[0]{Register}{$register_id};
    print XMLout($xml, KeepRoot =&gt; 1)
  }
  }
}
 else{
   say "$design_id: doesn't have $process_id";
  }
  }
 else
{
   say "$design_id does't exist in data";
}
}

